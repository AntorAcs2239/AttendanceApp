<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yessir">

    <application
        android:allowBackup="true"
        android:icon="@drawable/openscreen"
        android:label="@string/app_name"
        android:roundIcon="@drawable/openscreen"
        android:supportsRtl="true"
        android:theme="@style/Theme.YesSir">
        <activity android:name=".openscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RecordShowActivity" />
        <activity android:name=".StudentActivity" />
        <activity android:name=".MainActivity">

        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>